/include/ "mt7620a.dtsi"

/ {
	chosen {
		bootargs = "console=ttyS0,115200";
	};

	palmbus@10000000 {
		sysc@0 {
			ralink,gpiomux = "i2c", "jtag";
			ralink,uartmux = "gpio";
			ralink,wdtmux = <1>;
		};

		gpio0: gpio@600 {
			status = "okay";
		};

		gpio2: gpio@660 {
			status = "okay";
		};

		gpio3: gpio@688 {
			status = "okay";
		};

		spi@b00 {
			status = "okay";

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "w25q128";
				reg = <0 0>;
				linux,modalias = "m25p80", "w25q128";
				spi-max-frequency = <10000000>;

				partition@0 {
					label = "u-boot";
					reg = <0x0 0x30000>;
					read-only;
				};

				partition@30000 {
					label = "u-boot-env";
					reg = <0x30000 0x10000>;
					read-only;
				};

				factory: partition@40000 {
					label = "factory";
					reg = <0x40000 0x10000>;
					read-only;
				};

				bdinfo: partition@fe0000 {
					label = "bdinfo";
					reg = <0xfe0000 0x10000>;
					read-only;
				};

				backup: partition@ff0000 {
					label = "backup";
					reg = <0xff0000 0x10000>;
					read-only;
				};

				partition@50000 {
					label = "firmware";
					reg = <0x50000 0xf90000>;
				};
			};
		};
	};

	pinctrl {
		state_default: pinctrl0 {
			gpio {
				ralink,group = "mdio", "rgmii1", "uartf", "wled";
				ralink,function = "gpio";
			};
			nd_sd {
				ralink,group = "nd_sd";
				ralink,function = "sd";
			};						
		};
	};

	ehci@101c0000 {
		status = "okay";
	};

	ohci@101c1000 {
		status = "okay";
	};

	ethernet@10100000 {
		ralink,port-map = "wllll";
	};

	gsw@10110000 {
		ralink,port4 = "ephy";

		pinctrl-names = "default";
		pinctrl-0 = <&ephy_pins>;
	};

	sdhci@10130000 {
		status = "okay";
	};
	
	pcie@10140000 {
		status = "okay";
	};

	wmac@10180000 {
		ralink,mtd-eeprom = <&factory 0>;
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;
		reset {
			label = "reset";
			gpios = <&gpio0 12 1>;
			linux,code = <0x198>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";
		status {
			label = "hiwifi:green:status";
			gpios = <&gpio0 9 1>;
		};
		internet {
			label = "hiwifi:green:internet";
			gpios = <&gpio0 11 1>;
		};
		wlan1 {
			label = "hiwifi:green:wifi";
			gpios = <&gpio3 0 1>;
		};
	};
};
